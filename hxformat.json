{
    "lineEnds": {
        "leftCurly": "after",
        "rightCurly": "after",
        "emptyCurly": "noBreak",
        "objectLiteralCurly": {
            "leftCurly": "after"
        }
    },
    "sameLine": {
        "ifBody": "next",
        "elseBody": "next",
        "doWhileBody": "next",
        "tryBody": "next",
        "caseBody": "next",
        "forBody": "next"
    },
    "indentation": {
        "trailingWhitespace": true,
        "character": "    "
    }
}